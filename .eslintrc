{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"node": true
	},
	"rules": {
		"quotes": [2, "double"],
		"curly": [0],
		"no-mixed-requires": [0],
		"camelcase": [0],
		"eol-last": [0],
		"no-unused-vars": [0],
		"no-trailing-spaces": [0],
		"no-multi-spaces": [0],
		"key-spacing": [0],
		"no-underscore-dangle": [0]
	}
}
